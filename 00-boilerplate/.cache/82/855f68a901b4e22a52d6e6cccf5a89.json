: 12px 16px;\n                border-radius: 10px;\n                border: 1px solid #ddd;\n                box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.3) inset;\n                outline: none;\n                //Added by javaScript\n                &.error {\n                    border-color: $palette-error-main;\n                }\n            }\n            .error-message {\n                display: none;\n                &.error {\n                    display: block;\n                    color: $palette-error-main;\n                    margin-left: 16px;\n                }\n            }\n\n            .submit-button {\n                @include button;\n            } \n\n            \n\n         }\n\n         .footer{\n            font-weight: 300;\n            display: flex;\n            flex-direction: row;\n         &::before {\n             content: '';\n             background: url(../../core/content/img/secure_site.svg) no-repeat center center;\n             width: 20px;\n             margin-right: 5px;\n         }    \n        }\n    }\n\n    \n}\n","core/content/css/_palette.scss":"$palette-primary-main: #22c6cb;\n$palette-primary-contrast-text: white;\n$palette-secondary-main: #232c3b;\n$palette-secondary-contrast-text: white;\n$palette-error-main: #f44336;","core/content/css/_components.scss":"@import './palette';\n\n@mixin button(\n  $color: $palette-primary-main,\n  $contrast-text: $palette-primary-contrast-text\n) {\n  background-color: $color;\n  color: $contrast-text;\n  border: none;\n  padding: 12px 24px;\n  border-radius: 10px;\n  cursor: pointer;\n  font-weight: 900;\n  transition: all ease 0.2s;\n  text-decoration: none;\n\n  &:hover {\n    background-color: darken($color, 10%);\n  }\n\n  &:active {\n    box-shadow: inset 0 2px 0 0 rgba(0, 0, 0, 0.1),\n      inset 0 2px 2px 2px rgba(0, 0, 0, 0.2),\n      inset 0 0 2px 2px rgba(0, 0, 0, 0.1);\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"2a5232aa9101e4ca6c911c05bce03695","cacheData":{"env":