inal":{"line":143,"column":12},"generated":{"line":136,"column":2}},{"source":"pages/account-list/account-list.styles.scss","name":null,"original":{"line":143,"column":12},"generated":{"line":136,"column":25}},{"source":"pages/account-list/account-list.styles.scss","name":null,"original":{"line":144,"column":12},"generated":{"line":138,"column":0}},{"source":"pages/account-list/account-list.styles.scss","name":null,"original":{"line":145,"column":16},"generated":{"line":139,"column":2}},{"source":"pages/account-list/account-list.styles.scss","name":null,"original":{"line":145,"column":16},"generated":{"line":139,"column":13}},{"source":"pages/account-list/account-list.styles.scss","name":null,"original":{"line":146,"column":16},"generated":{"line":140,"column":2}},{"source":"pages/account-list/account-list.styles.scss","name":null,"original":{"line":146,"column":16},"generated":{"line":140,"column":18}}],"sources":{"pages/account-list/account-list.styles.scss":"@import '../../core/content/css/palette';\n@import '../../core/content/css/breakpoints';\n@import '../../core/content/css/components';\n.root {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding: 0 10%;\n\n    & > :nth-child(n){\n        width: 100vw;\n        display: flex;\n        flex-direction: row;\n        justify-content: center;\n        & > :nth-child(n) {\n            width: 100%;\n            max-width: $breakpoints-lg;\n        }\n    }\n\n    header{\n        background-color: $palette-primary-main;\n        padding: 40px 0;\n        .header-subcontainer {\n            max-width: $breakpoints-lg;\n            width: 100%;\n            display: flex;\n            flex-direction: row;\n            justify-content: space-between;\n            align-items: center;\n            padding: 0 16px;\n        }\n        img {\n           width: 100%;\n           max-width: 200px;\n        }\n\n        .user {\n            color: $palette-secondary-contrast-text;\n            display: flex;\n            flex-direction: row;\n            gap: 10px;\n            &::before {\n                content: '';\n                background: url(../../core/content/img/icon-user.svg) no-repeat center center;\n                width: 20px;\n            }\n        }\n    } \n\n\n    nav {\n        background-color: $palette-secondary-main;\n        \n        ul {\n          margin-top: 10px;\n          margin-bottom: 0px;\n          padding-top: 0px;  \n          list-style: none;\n          display: flex;\n          flex-direction: row;\n          justify-content: center;\n          gap:40px\n        }\n        & > :nth-child(n) {\n            color: $palette-secondary-contrast-text;\n        }\n        a {\n            color: $palette-secondary-contrast-text;\n            text-decoration: none;\n        }\n        .selected {\n            border-bottom: 5px solid $palette-primary-main ;\n        }\n    }\n\n    section {\n        .title-container {\n            display: flex;\n            flex-direction: row;\n            justify-content: space-between;\n            border-bottom: 5px solid $palette-secondary-main;\n            padding-bottom: 5px;\n            margin-top: 30px;\n            .title {\n                margin: 0px;\n                font-weight: 300;\n                font-size: 2.2rem;\n            }\n            .new-account-button {\n                @include button;\n            }\n        }\n\n        .account-list {\n            width: 100%;\n            margin-top: 5px;\n            color: $palette-secondary-main;\n            thead tr{\n                background-color: #ccc;\n                font-weight: 900;\n            }\n\n            tbody {\n                tr:nth-child(even) {\n                    background-color: #eee;\n                    font-weight: 300;\n                }\n\n                td {\n                    padding: 10px;\n                    font-size: 0.8rem;\n\n                }\n                .iban a{\n                    color: $palette-secondary-main;\n                    font-weight: 600;\n\n                }\n                .align-right {\n                    text-align: right;\n                }\n                .select {\n                    padding: 8px 12px;\n                    border-radius: 10px;\n                    border: 1px solid black;\n                    width: 100%;\n                    max-width: 200px;\n                    box-shadow: 0px 1em 2em -1.5em rgba(0, 0, 0, 0.5);\n                }\n            }\n\n        }\n\n    }\n    footer{\n        .footer-subcontainer {\n            background-color: $palette-secondary-main;\n            padding: 80px;\n            margin-top: 100px;\n            display: flex;\n            flex-direction: row;\n            justify-content: center;\n            img {\n                width: 100%;\n                max-width: 200px;\n            }\n        }\n        \n    } \n}\n\n","core/content/css/_breakpoints.scss":"$breakpoints-md: 900px;\n$breakpoints-lg: 1200px;","core/content/css/_palette.scss":"$palette-primary-main: #22c6cb;\n$palette-primary-contrast-text: white;\n$palette-secondary-main: #232c3b;\n$palette-secondary-contrast-text: white;\n$palette-error-main: #f44336;","core/content/css/_components.scss":"@import './palette';\n\n@mixin button(\n  $color: $palette-primary-main,\n  $contrast-text: $palette-primary-contrast-text\n) {\n  background-color: $color;\n  color: $contrast-text;\n  border: none;\n  padding: 12px 24px;\n  border-radius: 10px;\n  cursor: pointer;\n  font-weight: 900;\n  transition: all ease 0.2s;\n  text-decoration: none;\n\n  &:hover {\n    background-color: darken($color, 10%);\n  }\n\n  &:active {\n    box-shadow: inset 0 2px 0 0 rgba(0, 0, 0, 0.1),\n      inset 0 2px 2px 2px rgba(0, 0, 0, 0.2),\n      inset 0 0 2px 2px rgba(0, 0, 0, 0.1);\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"77bfb88c7ae036168e3d3e29bdfb13c5","cacheData":{"env":{}}}