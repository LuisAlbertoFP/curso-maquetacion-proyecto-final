ogin) { */\nbody {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding: 0 10%;\n  \n    & > :nth-child(n){\n        width: 100vw;\n        display: flex;\n        flex-direction: row;\n        justify-content: center;\n        & > :nth-child(n) {\n            width: 100%;\n            max-width: $breakpoints-lg;\n        }\n    }\n      /*************\n      Cabecera página\n      **************/\n      header{\n        background-color: $palette-primary-main;\n        padding: 40px 0;\n  \n        & > div{\n            max-width: $breakpoints-lg;\n            width: 100%;\n            display: flex;\n            flex-direction: row;\n            justify-content: space-between;\n            align-items: center;\n            padding: 0 16px;\n  \n            & > p {\n              color: $palette-secondary-contrast-text;\n              display: flex;\n              flex-direction: row;\n              gap: 10px;\n              &::before {\n                  content: '';\n                  /* background: url(../img/icon-user.svg) no-repeat center center; */\n                  background: url('../../core/content/img/icon-user.svg') no-repeat center center;\n                  width: 20px;\n              }\n          }\n          a, /* <=para que funcione en firefox*/\n          img {\n            width: 100%;\n            max-width: 200px;\n          }  \n        }\n        \n      } \n      /*************\n      Barra de navegación\n      **************/\n      nav {\n        background-color: $palette-secondary-main;\n        \n        ul {\n          margin-top: 10px;\n          margin-bottom: 0px;\n          padding-top: 0px;  \n          list-style: none;\n          display: flex;\n          flex-direction: row;\n          justify-content: center;\n          gap:40px;\n          li.selected {\n            border-bottom: 5px solid $palette-primary-main ;\n          }\n        }\n        & > :nth-child(n) {\n            color: $palette-secondary-contrast-text;\n        }\n        a {\n            color: $palette-secondary-contrast-text;\n            text-decoration: none;\n        }\n        \n    }\n      /************\n      Sección principal\n      ************/\n      section {\n        & > div > div{ \n            display: flex;\n            flex-direction: row;\n            justify-content: space-between;\n            border-bottom: 5px solid $palette-secondary-main;\n            padding-bottom: 5px;\n            margin-top: 30px;\n            h1 {\n                margin: 0px;\n                font-weight: 300;\n                font-size: 2.2rem;\n            }\n            p {\n              font-weight: 800;\n              color: $palette-secondary-main;\n              font-size: 0.8rem;\n            }\n            a {\n                @include button;\n            }\n        }\n      }\n\n\n    /*****\n    PIE DE PÁGINA\n    *****/\n    footer{\n        width: 100vw;\n        margin-top: 100px;\n        background-color: $palette-secondary-main;\n        & > div {\n            padding: 80px;\n            display: flex;\n            flex-direction: row;\n            justify-content: center;\n            img {\n                width: 100%;\n                max-width: 200px;\n            }\n        }\n      }  \n  }","core/content/css/_breakpoints.scss":"$breakpoints-md: 900px;\n$breakpoints-lg: 1200px;","core/content/css/_palette.scss":"$palette-primary-main: #22c6cb;\n$palette-primary-contrast-text: white;\n$palette-secondary-main: #232c3b;\n$palette-secondary-contrast-text: white;\n$palette-error-main: #f44336;","core/content/css/_components.scss":"@import './palette';\n\n@mixin button(\n  $color: $palette-primary-main,\n  $contrast-text: $palette-primary-contrast-text\n) {\n  background-color: $color;\n  color: $contrast-text;\n  border: none;\n  padding: 12px 24px;\n  border-radius: 10px;\n  cursor: pointer;\n  font-weight: 900;\n  transition: all ease 0.2s;\n  text-decoration: none;\n\n  &:hover {\n    background-color: darken($color, 10%);\n  }\n\n  &:active {\n    box-shadow: inset 0 2px 0 0 rgba(0, 0, 0, 0.1),\n      inset 0 2px 2px 2px rgba(0, 0, 0, 0.2),\n      inset 0 0 2px 2px rgba(0, 0, 0, 0.1);\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"1098f9baaaadd859733eff68777d20ad","cacheData":{"env":{}}}